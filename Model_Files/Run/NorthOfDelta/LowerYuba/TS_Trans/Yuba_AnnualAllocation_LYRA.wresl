!**********************************************************************************************************************************************************
!  Define maximum reservoir storage levels for purposes of forecasting operations April-September
!***********************************************************************************************************************************************************

define S_NBBlevel4_APR {select target from res_level where res_num=93,level=4,month=7}
define S_NBBlevel4_MAY {select target from res_level where res_num=93,level=4,month=8}
define S_NBBlevel4_JUN {select target from res_level where res_num=93,level=4,month=9}
define S_NBBlevel4_JUL {select target from res_level where res_num=93,level=4,month=10}
define S_NBBlevel4_AUG {select target from res_level where res_num=93,level=4,month=11}
define S_NBBlevel4_SEP {select target from res_level where res_num=93,level=4,month=12}

!**********************************************************************************************************************************************************
!  Apply a large penalty if the forecasted storage is greater than top of conservation
!***********************************************************************************************************************************************************

goal set_maxS_NBB_APR {lhs S_NBB_APR rhs S_NBBlevel4_APR lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}
goal set_maxS_NBB_MAY {lhs S_NBB_MAY rhs S_NBBlevel4_MAY lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}
goal set_maxS_NBB_JUN {lhs S_NBB_JUN rhs S_NBBlevel4_JUN lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}
goal set_maxS_NBB_JUL {lhs S_NBB_JUL rhs S_NBBlevel4_JUL lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}
goal set_maxS_NBB_AUG {lhs S_NBB_AUG rhs S_NBBlevel4_AUG lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}
goal set_maxS_NBB_SEP {lhs S_NBB_SEP rhs S_NBBlevel4_SEP lhs>rhs penalty 100000.*taf_cfs lhs<rhs penalty 0.}

