!**********************************************************************************************************************************************************
! Write continuity equations based on a simplified network of tyhe following nodes
! New Bullards Bar; Englebright; and Daguerrepoint Dam
! C_NBB = release from New Bullards Bar
! C_EB = release from Englebright
! C_DP = flow below Daguerre Point Diversion Dam
!*********************************************************************************************************************************************************** 

!continuity for New Bullards Bar
goal continuityNBB_APR {I_NBB_APR-C_NBB_APR=S_NBB_APR*taf_cfs-S_NBB_MAR*taf_cfs}
goal continuityNBB_MAY {I_NBB_MAY-C_NBB_MAY=S_NBB_MAY*taf_cfs*30/31-S_NBB_APR*taf_cfs*30/31}
goal continuityNBB_JUN {I_NBB_JUN-C_NBB_JUN=S_NBB_JUN*taf_cfs-S_NBB_MAY*taf_cfs}
goal continuityNBB_JUL {I_NBB_JUL-C_NBB_JUL=S_NBB_JUL*taf_cfs*30/31-S_NBB_JUN*taf_cfs*30/31}
goal continuityNBB_AUG {I_NBB_AUG-C_NBB_AUG=S_NBB_AUG*taf_cfs*30/31-S_NBB_JUL*taf_cfs*30/31}
goal continuityNBB_SEP {I_NBB_SEP-C_NBB_SEP=S_NBB_SEP*taf_cfs-S_NBB_AUG*taf_cfs}

!continuity for Englebright
goal continuityEB_APR {I_EB_APR+C_NBB_APR=C_EB_APR} 
goal continuityEB_MAY {I_EB_MAY+C_NBB_MAY=C_EB_MAY} 
goal continuityEB_JUN {I_EB_JUN+C_NBB_JUN=C_EB_JUN} 
goal continuityEB_JUL {I_EB_JUL+C_NBB_JUL=C_EB_JUL} 
goal continuityEB_AUG {I_EB_AUG+C_NBB_AUG=C_EB_AUG} 
goal continuityEB_SEP {I_EB_SEP+C_NBB_SEP=C_EB_SEP} 

!continuity for Daguerre Point
goal continuityDGR_APR {I_DGR_APR+C_EB_APR=C_DGR_APR+D_DGR_APR}
goal continuityDGR_MAY {I_DGR_MAY+C_EB_MAY=C_DGR_MAY+D_DGR_MAY}
goal continuityDGR_JUN {I_DGR_JUN+C_EB_JUN=C_DGR_JUN+D_DGR_JUN}
goal continuityDGR_JUL {I_DGR_JUL+C_EB_JUL=C_DGR_JUL+D_DGR_JUL}
goal continuityDGR_AUG {I_DGR_AUG+C_EB_AUG=C_DGR_AUG+D_DGR_AUG}
goal continuityDGR_SEP {I_DGR_SEP+C_EB_SEP=C_DGR_SEP+D_DGR_SEP}

! Daguerre Point deliveries based on water rights and allocations
! Demand is approximately 80% of full entitlement
! The adjustment should drop to zero once the allocation drops below 1.
! Code below adjusts the contract/water right when allocations are 1 (ie non-binding, and crop water demand is binding)
! and removes adjustment when allocations are less than 1 (ie binding and crop water demand is non-binding)
! Goal of code: when YCWA_Alloc <1, int_YCWA_alloc2=1, int_YCWA_alloc=0
!				when YCWA_Alloc =1, int_YCWA_alloc2=0, int_YCWA_alloc=1

define int_YCWA_alloc  {integer std kind 'INTEGER' units 'NONE'}
define int_YCWA_alloc2 {integer std kind 'INTEGER' units 'NONE'} ! Fixed to be properly defined as an integer, NSO 06/21/2022
goal setint_YCWA_alloc1 {int_YCWA_alloc2 < 99999 - 99999*YCWA_Alloc}
goal setint_YCWA_alloc2 {int_YCWA_alloc2 > 1-YCWA_Alloc}
goal setint_YCWA_alloc3 {int_YCWA_alloc = 1-int_YCWA_alloc2}

goal allocation_APR2 {D_DGR_APR=D_DGR_APR1+ D_DGR_APR2}
goal allocation_MAY2 {D_DGR_MAY=D_DGR_MAY1+ D_DGR_MAY2}
goal allocation_JUN2 {D_DGR_JUN=D_DGR_JUN1+ D_DGR_JUN2}
goal allocation_JUL2 {D_DGR_JUL=D_DGR_JUL1+ D_DGR_JUL2}
goal allocation_AUG2 {D_DGR_AUG=D_DGR_AUG1+ D_DGR_AUG2}
goal allocation_SEP2 {D_DGR_SEP=D_DGR_SEP1+ D_DGR_SEP2}

goal allocation_APR1 {D_DGR_APR1=AprilFlag*YCWA_Alloc*CL_DGR   + AprilFlag*WR_DGR   }
goal allocation_MAY1 {D_DGR_MAY1=AprilFlag*YCWA_Alloc*CL_DGR(1)+ AprilFlag*WR_DGR(1)}
goal allocation_JUN1 {D_DGR_JUN1=AprilFlag*YCWA_Alloc*CL_DGR(2)+ AprilFlag*WR_DGR(2)}
goal allocation_JUL1 {D_DGR_JUL1=AprilFlag*YCWA_Alloc*CL_DGR(3)+ AprilFlag*WR_DGR(3)}
goal allocation_AUG1 {D_DGR_AUG1=AprilFlag*YCWA_Alloc*CL_DGR(4)+ AprilFlag*WR_DGR(4)}
goal allocation_SEP1 {D_DGR_SEP1=AprilFlag*YCWA_Alloc*CL_DGR(5)+ AprilFlag*WR_DGR(5)}

goal allocation_APR {D_DGR_APR=AprilFlag*YCWA_Alloc*CL_DGR   + AprilFlag*WR_DGR   -  5*taf_cfs   *AprilFlag*int_YCWA_alloc2}
goal allocation_MAY {D_DGR_MAY=AprilFlag*YCWA_Alloc*CL_DGR(1)+ AprilFlag*WR_DGR(1)- 10*taf_cfs(1)*AprilFlag*int_YCWA_alloc2}
goal allocation_JUN {D_DGR_JUN=AprilFlag*YCWA_Alloc*CL_DGR(2)+ AprilFlag*WR_DGR(2)- 10*taf_cfs(2)*AprilFlag*int_YCWA_alloc2}
goal allocation_JUL {D_DGR_JUL=AprilFlag*YCWA_Alloc*CL_DGR(3)+ AprilFlag*WR_DGR(3)- 10*taf_cfs(3)*AprilFlag*int_YCWA_alloc2}
goal allocation_AUG {D_DGR_AUG=AprilFlag*YCWA_Alloc*CL_DGR(4)+ AprilFlag*WR_DGR(4)- 10*taf_cfs(4)*AprilFlag*int_YCWA_alloc2}
goal allocation_SEP {D_DGR_SEP=AprilFlag*YCWA_Alloc*CL_DGR(5)+ AprilFlag*WR_DGR(5)-  5*taf_cfs(5)*AprilFlag*int_YCWA_alloc2}

define D_DGR_total {std kind 'DELIVERY' units 'TAF'}
goal setD_DGR_total {D_DGR_total = D_DGR_APR*cfs_taf + D_DGR_MAY*cfs_taf(1) + D_DGR_JUN*cfs_taf(2) + D_DGR_JUL*cfs_taf(3)
	+ D_DGR_AUG*cfs_taf(4)+ D_DGR_SEP*cfs_taf(5)}

